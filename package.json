{
  "name": "remirror-experimental-extension-tables",
  "license": "MIT",
  "version": "0.1.2",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext '.js,.jsx,.ts,.tsx'",
    "dev": "parcel index.html",
    "build:ts": "mkdir -p lib && rm -rf lib && tsc -p tsconfig.build.json",
    "build:scss": "touch style/table.css && rm style/*.css* && sass style/table.scss style/table.css",
    "publish": "yarn build:ts && yarn build:scss && yarn npm publish"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "style",
    "tsconfig.json"
  ],
  "browserslist": [
    "> 1%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/node": "^14.14.22",
    "@types/react": "*",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "^1.12.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.5",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.12"
  },
  "dependencies": {
    "@remirror/core": "1.0.0-next.60",
    "@remirror/core-utils": "1.0.0-next.60",
    "@remirror/dev": "1.0.0-next.60",
    "@remirror/extension-emoji": "1.0.0-next.60",
    "@remirror/extension-mention": "1.0.0-next.60",
    "@remirror/extension-mention-atom": "1.0.0-next.60",
    "@remirror/extension-paragraph": "1.0.0-next.60",
    "@remirror/extension-react-component": "1.0.0-next.60",
    "@remirror/extension-text": "1.0.0-next.60",
    "@remirror/pm": "1.0.0-next.60",
    "@remirror/preset-core": "1.0.0-next.60",
    "@remirror/preset-table": "1.0.0-next.60",
    "@remirror/react": "1.0.0-next.60",
    "@remirror/react-hooks": "1.0.0-next.60",
    "jsx-dom": "^7.0.0-beta.0",
    "prosemirror-tables": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^16 || ^17",
    "react-dom": "^16 || ^17"
  }
}
